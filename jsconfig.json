{
	"compilerOptions": {
			"target": "ES2022",
			"module": "ESNext",
			"moduleResolution": "node",
			"checkJs": true,
			"allowJs": true,
			"noEmit": true,
			"strict": true,
			"baseUrl": "./",
			"paths": {
				"/*": ["www/*"]
			}
	},
	"include": [
		"www/**/*.js",
		"test/**/*.js"
, "bin/accept-snapshots.js"	],
	"exclude": [
		"node_modules"
	]
}
